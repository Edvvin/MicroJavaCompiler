program SwitchesAndWhiles
const int N = 10;
{
	void main()
	int i,j;
	{
		i = 0;
		j = 0;
		do{
			do{
			if(i>0)
				break;
				j = switch(j){
					case 0:
					print('H');
					yield j+1;
					case 1:
					print('e');
					yield j+1;
					case 2:
					case 3:
					print('l');
					yield j+1;
					default:
					print(switch(i){case 1: yield 'H'; default: yield 'o';});
					yield j+1;
				};
			}while(j <= 4);
			print(' ');
			i++;
		}while(i<N);
	}

}